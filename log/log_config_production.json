{
  "version": 1,
  "formatters": {"default": {
      "format": "%(asctime)s %(levelname)s in %(module)s: %(message)s",
      "datefmt": "%y%m%d %H:%M:%S"
  }},
  "handlers": {
      "file": {
          "class": "logging.FileHandler",
          "filename": "molmod.log",
          "formatter": "default"
      }
  },
  "root": {
      "level": "WARNING",
      "handlers": ["file"]
  }
}
