{
  "version": 1,
  "formatters": {"default": {
      "format": "%(asctime)s %(levelname)s in %(module)s: %(message)s",
      "datefmt": "%y%m%d %H:%M:%S"
  }},
  "handlers": {
      "wsgi": {
          "class": "logging.StreamHandler",
          "stream": "ext://flask.logging.wsgi_errors_stream",
          "formatter": "default"
      },
      "downloads": {
          "class": "logging.FileHandler",
          "filename": "dummy.log",
          "formatter": "default"
      }
  },
  "root": {
      "level": "WARNING",
      "handlers": ["wsgi"]
  },
  "loggers": {
      "downloads": {
          "level": "INFO",
          "handlers": ["downloads"]
      }
  }
}
